<div class="tech-btm">
    <h4>Mail bültenine abone ol</h4>
    <p>Yeni bloglardan ilk siz haberdar olun</p>
  <form method="post">
             
    <input type="email" id="txtMail" name="Mail" placeholder="Mail" required="">
        <input id="btnGonder" type="submit" value="Abone Ol">
   </form>
</div>  

<script type="text/javascript">
    $("#btnGonder").click(function () {
        let values={
            Mail: $("#txtMail").val()
        };

        $.ajax({
            type: "post",
            url: "/NewsLetter/SubscribeMail/",
            data: values,
            success: function (func) {
                let result=jQuery.parseJSON(func);
            }
        });
    });
</script>

<script src="https://code.jquery.com/jquery-3.5.1.min.js" type="text/javascript"></script>

